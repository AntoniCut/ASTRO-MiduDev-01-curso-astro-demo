{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "./src/components/*"
      ],
      "@/layouts/*": [
        "./src/layouts/*"
      ],
      "@/scripts/*": [
        "./src/scripts/*"
      ],
      "@/services/*": [
        "./src/services/*"
      ],
      "@/styles/*": [
        "./src/styles/*"
      ],
      "@/types/*": [
        "./src/types/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}